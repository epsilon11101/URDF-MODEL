<?xml version="1.0"?>
<robot name="prueba" 
    xmlns:xacro="http://www.ros.org/wiki/xacro">



    <xacro:macro name="inertia_cal" params="xx xy xz yy yz zz mass vol">

        <inertial>

            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="${mass}"/>
            <inertia ixx="${xx*mass/vol}" ixy="${xy*mass/vol}" ixz="${xz*mass/vol}" iyy="${yy*mass/vol}" iyz="${yz*mass/vol}" izz="${zz*mass/vol}"/>
        </inertial>
    </xacro:macro>


    <link name="world"/>
    <link name="base_link">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/link1.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        </visual>

        <xacro:inertia_cal xx="1.000000" xy="0.000011" xz="-0.000014" yy="0.985006" yz="-0.172521" zz="0.985006" mass="0.000340" vol="0.000727"/>
    </link>



    <joint name="fixed" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <!-- -  - - - - - - - SHOULDER- - - - - - - - - - - -  -->
    <link name="link1">

        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/link2.dae" scale="1 1 1"/>
            </geometry>
            <material name="orange">
                <color rgba="${255/255} ${140/255} ${0/255} 1.0"/>
            </material>
            <origin xyz="0.0 -0.0045 -0.03" rpy="0.0 0.0 0.0"/>
        </visual>

        <collision>
            <origin xyz="0.0 -0.0045 -0.03" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/boxL2.dae" scale="1 1 1"/>
            </geometry>
        </collision>

        <xacro:inertia_cal xx="0.998722" xy="-0.050154" xz="0.006288" yy="0.947029" yz="-0.317303" zz="0.948303" mass="0.000340" vol="0.000340"/>


    </link>


    <joint name="shoulder" type="revolute">

        <axis xyz="1 0 0"/>
        <limit effort="200" lower="-0.20" upper="2.74" velocity="1.0"/>

        <parent link="base_link"/>
        <child link="link1"/>
        <origin xyz="0.0 0.0045 0.03" rpy="0.0 0.0 0.0"/>

    </joint>


    <!-- -  - - - - - - - - - ELBOW - - - - - - - - -  -->

    <link name="link2">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/link3.dae" scale="1 1 1"/>
            </geometry>
            <material name="orange">
                <color rgba="${255/255} ${140/255} ${0/255} 1.0"/>
            </material>
            <origin xyz="0.0594107529521 -0.5709   -0.223 " rpy="0.0 0.0 0.0"/>
        </visual>

        <collision>
            <origin xyz="0.0594107529521 -0.5709   -0.223 " rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/boxL3.dae" scale="1 1 1"/>
            </geometry>
        </collision>

        <xacro:inertia_cal xx="0.998935" xy="0.042751" xz="-0.017379" yy="0.788307" yz="0.614850" zz="0.788453" mass="0.000303" vol="0.000303"/>

    </link>

    <joint name="elbow" type="continuous">
        <axis xyz="1.0 0.0 0.0"/>
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="-0.0594107529521 0.5709   0.19 " rpy="0.0 0.0 0.0"/>
    </joint>


    <!-- -  - - - - - - - -WRIST- - - - - - - - - - -  -->
    <link name="link3">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/link4.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/boxL4.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>


    </link>

    <joint name="wrist" type="revolute">
        <limit lower="-0.73" upper="3.14" effort="200.0" velocity="1.0"/>
        <axis xyz="1.0 0.0 0.0"/>
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="0.01 -0.365001 0.28 " rpy="0.0 0.0 0.0"/>
    </joint>

    <!-- - - - - - - - - - -HAND - - - - - - - - - -->
    <link name="link4">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/base.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="-0.001 0.01 -0.021" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <origin xyz="-0.001 0.01 -0.021" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/base.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>


    </link>

    <joint name="hand" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0 0.055 0.125" rpy="-2.0 0.0 0.0"/>
    </joint>

    <!-- - - - - - - - - - - KNUCLES - - - - - - - - - -->


    <link name="link5">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte1.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>

        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte1.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>
    </link>


    <joint name="right_knuckles" type="revolute">
        <parent link="link4"/>
        <child link="link5"/>
        <origin xyz="0.109902  -0.074139 -0.01 " rpy="0 0 0.0"/>
        <axis xyz="0 0 1 "/>
        <limit effort="200" lower="0.53" upper="1.52" velocity="1.0"/>
    </joint>


    <link name="link6">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte1.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte1.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>
    </link>


    <joint name="left_knuckles" type="revolute">
        <parent link="link4"/>
        <child link="link6"/>
        <origin xyz="-0.109902  -0.074139 -0.01 " rpy="0 0 0.0"/>
        <axis xyz="0 0 1 "/>
        <limit effort="200" lower="-1.52" upper="-0.43" velocity="1.0"/>
        <!-- <mimic joint="right_knuckles" multiplier="-1" offset="0"/> -->
    </joint>
    <!-- - - - - - - - - - - FINGERS - - - - - - - - - -->
    <link name="link7">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/finerr.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/finerr.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>

    </link>


    <joint name="left_finger" type="revolute">
        <parent link="link6"/>
        <child link="link7"/>
        <origin xyz="0.000606 -0.097249 0.027551  " rpy="0 0 -0.5"/>
        <axis xyz="0 0 1 "/>

        <limit effort="200" lower="0.52" upper="1" velocity="1.0"/>
        <!-- <mimic joint="right_knuckles" multiplier="1" offset="0.6"/> -->
    </joint>

    <link name="link8">
        <visual>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte2.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue">
                <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
            </material>
            <origin xyz="0 0 0.0" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://odam_arm/meshes/GRIPPER_DAE/parte2.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertia_cal xx="0.998679" xy="-0.000695" xz="0.051389" yy="0.913907" yz="0.405419" zz="0.912685" mass="0.000175" vol="0.000175"/>

    </link>


    <joint name="right_finger" type="revolute">
        <parent link="link5"/>
        <child link="link8"/>
        <origin xyz="-0.000606 -0.097249 0.027551  " rpy="0 0 0.5"/>
        <axis xyz="0 0 1 "/>

        <limit effort="200" lower="-1" upper="-0.52" velocity="1.0"/>
        <!-- <mimic joint="right_knuckles" multiplier="-1" offset="-0.6"/> -->
    </joint>


    <!-- COLORS -->

    <gazebo reference="base_link">
        <material>Gazebo/Blue</material>
    </gazebo>

    <gazebo reference="link1">
        <material>Gazebo/Orange</material>
    </gazebo>

    <gazebo reference="link2">
        <material>Gazebo/Orange</material>
    </gazebo>


    <gazebo reference="link3">
        <material>Gazebo/Blue</material>
    </gazebo>

        <gazebo reference="link4">
        <material>Gazebo/Orange</material>
    </gazebo>

        <gazebo reference="link5">
        <material>Gazebo/Orange</material>
    </gazebo>

        <gazebo reference="link6">
        <material>Gazebo/Orange</material>
    </gazebo>

        <gazebo reference="link7">
        <material>Gazebo/Blue</material>
    </gazebo>

        <gazebo reference="link8">
        <material>Gazebo/Blue</material>
    </gazebo>




    <!--  TRANSMISSION -->
    
 <transmission name="trans_shoulder">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="shoulder">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="shoulder_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_elbow">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="elbow_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_wrist">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_hand">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="hand">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="hand_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_right_knuckles">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="right_knuckles">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="right_knuckles_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_left_knuckles">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="left_knuckles">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="left_knuckles_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_left_finger">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="left_finger">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="left_finger_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_right_finger">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="right_finger">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="right_finger_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <!--  PLUGINS -->


    <gazebo>
        <plugin name="control" filename="libgazebo_ros_control.so">
            <legacyModeNS>true</legacyModeNS>
        </plugin>
    </gazebo>

    <gazebo>
        <plugin filename="libgazebo_ros_joint_state_publisher.so" name="joint_state_publisher">
            <jointName>shoulder,elbow,wrist,hand,right_knuckles,left_knuckles,right_finger,left_finger</jointName>
        </plugin>
    </gazebo>


</robot>


















